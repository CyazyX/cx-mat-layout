{
    "name": "cx-mat-layout-demo",
    "version": "1.2.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build_lib": "ng build --prod --project @cyazyx/cx-mat-layout",
        "bump_version": "node tools/version-increment.js",
        "gitversion": "node tools/git-version-all.js",
        "dependency_update": "node tools/version-local-clean.js",
        "npm_pack": "cd dist/cyazyx/cx-mat-layout && npm pack",
        "package": "npm run build_lib && npm run npm_pack",
        "prepare_deploy": "npm run dependency_update && npm run bump_version && npm run build_lib && npm run gitversion",
        "install_local": "npm run package && node tools/install-local.js",
        "publish_public": "cd dist/cyazyx/cx-mat-layout && npm publish --access public",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": false,
    "license": "MIT",
    "repository": "https://github.com/CyazyX/cx-mat-layout",
    "dependencies": {
        "@angular/animations": "~11.0.2",
        "@angular/cdk": "^11.0.1",
        "@angular/common": "~11.0.2",
        "@angular/compiler": "~11.0.2",
        "@angular/core": "~11.0.2",
        "@angular/forms": "~11.0.2",
        "@angular/material": "^11.0.1",
        "@angular/platform-browser": "~11.0.2",
        "@angular/platform-browser-dynamic": "~11.0.2",
        "@angular/router": "~11.0.2",
        "@cyazyx/cx-mat-layout": "",
        "rxjs": "~6.6.3",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1100.2",
        "@angular/cli": "~11.0.2",
        "@angular/compiler-cli": "~11.0.2",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^14.14.12",
        "codelyzer": "^6.0.0",
        "fs-extra": "^9.0.1",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.1.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "^11.0.3",
        "protractor": "~7.0.0",
        "ts-node": "~8.10.2",
        "tslint": "~6.1.0",
        "typescript": "~4.0.5"
    }
}